{
   "name": "Voat Speed Dial Extension",
   "developer": "EvaUnit02",
   "version": "0.5",
   "manifest_version": 2,
   "permissions": ["speeddial"],
   "background": {"page": "extension.html"},
   "options_page": "options.html",
   "speeddial":
   {
      "title": "Voat Speeddial",
      "url": "https://teamm3.com/",
      "size_mode": "auto_zoom"
   },
   "icons":
   {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
   },
   "permissions":
   [
      "http://*.voat.co/*",
      "https://*.voat.co/*",
      "http://fakevout.azurewebsites.net/*",
      "tabs"
   ]
}